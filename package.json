{
  "name": "cardboard-butler",
  "version": "2.0.0",
  "description": "Find the boardgames from BGG to play",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "sass",
    "bgg",
    "boardgames"
  ],
  "author": "Philip Kristoffersen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PhilipK/CardboardButler.git"
  },
  "bugs": {
    "url": "https://github.com/PhilipK/CardboardButler/issues"
  },
  "homepage": "https://github.com/PhilipK/CardboardButler",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "tslint './src/**/*.ts*' --format stylish --force",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.9.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^11.13.20",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "express": "^4.17.1",
    "fetch-mock": "^7.3.9",
    "fetchmock": "^1.0.1",
    "file-loader": "^3.0.1",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.9.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "react-hot-loader": "^4.12.12",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "semantic-ui-react": "^0.88.0",
    "ts-jest": "^24.0.2",
    "xml-js": "^1.6.11"
  }
